<!doctype html>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
<title>
    Placement Procedure
</title>

    <body>
<div class="container-fluid" style="padding-top: 14px;background-color: gray">
    <nav class="navbar navbar-inverse" style="background-color: black">

        <div class="nav nav-pills" style="color: orange;font-weight: 600;font-family: cursive; font-size: 17px;padding-bottom: 3px">

            <button type="button" class="navbar-toggle" data-toggle="collapse" >
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <div class="collapse navbar-collapse" id="myNavbar">
                <ul class="nav navbar-nav">
                    <li><a href="/index.html">Placement Portal</a></li>
                    <li ><a href="/student_details">Student's Details</a></li>
                    <li><a href="/company_details">Companies Visiting</a></li>
                    <li><a href="/unregister_company">Unregister Company</a></li>
                    <li><a href="/unregister_student">Unregister Student</a></li>
                    <li><a href="/update_student">Update Student</a></li>
                </ul>
                
            </div>
        </div>
    </nav>
</div>
<br/>
<div class="container-fluid" style="background-color: aliceblue">
        <div class="row">
            <div class="jumbotron">
                <div class="col-lg-12" style="background-color: lightgray">
                        
                        <div class="jumbotron; col-lg-12">
                            <h4>Registered Company's List:</h4><hr style="border:groove;color: black">
                            <table style="width:100%" class="quotes">
                                <tr>
                                    <th>Company's Name</th>
                                    <th>Department</th> 
                                    <th>CTC</th>
                                    <th>Type</th>
                                    <th>Establishment Year</th>
                                    <th>Country</th>
                                    <th>Nature of Business</th>
                                    <th>CGPA Cutoff</th>
                                    <th>Contact Number</th>
                                </tr>
                                
                                    <% for(var i=0; i<quotes.length; i++) {%>
                                        <tr >
                                        <td><span><%= quotes[i].company_name %></span></td>
                                        <td><span><%= quotes[i].department %></span></td>
                                        <td><span><%= quotes[i].ctc %></span></td>
                                        <td><span><%= quotes[i].type %></span></td>
                                        <td><span><%= quotes[i].estabYear %></span></td>
                                        <td><span><%= quotes[i].country %></span></td>
                                        <td><span><%= quotes[i].natureOfBusiness %></span></td>
                                        <td><span><%= quotes[i].cgpaCutOff %></span></td>
                                        <td><span><%= quotes[i].contact %></span></td>
                                    </tr>
                                  <% } %>
                                
            </table>
                    </div>
                </div>
            </div>
        </div>
</div>
</body>
</html>